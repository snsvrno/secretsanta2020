							"condition": [
								0,
								"mechanic_who"
							]
						}
					]
				},
				{
					"id": "mechanic_who_response",
					"target": "player",
					"speaker": "mechanic",
					"text": "The Owner.",
					"action": "mechanic_whatiscar"
				},
				{
					"id": "mechanic_whoisthat_response",
					"target": "player",
					"speaker": "mechanic",
					"text": "/Looks at you suspiciously../\\nI can't be giving that kind of information out to anyone.",
					"action": "mechanic_whatiscar"
				},
				{
					"id": "mechanic_fixit_response",
					"target": "player",
					"speaker": "mechanic",
					"text": "Of course I can!\\nWho do you think I am?",
					"action": "mechanic_whoisme"
				},
				{
					"id": "mechanic_whoisme_idk",
					"speaker": "player",
					"target": "mechanic",
					"text": "I don't know.",
					"chain": "mechanic_whoisme_idk_response",
					"display": "Not sure."
				},
				{
					"id": "mechanic_whoisme_xxx",
					"speaker": "player",
					"target": "mechanic",
					"text": "$mechanic$",
					"chain": "mechanic_whoisme_xxx_response",
					"display": "$mechanic$",
